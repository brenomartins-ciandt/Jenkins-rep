pipeline {
    agent any
    
	stages {
        stage ('Compose Stage'){
            steps {
                // sh 'pwd'
                // sh 'docker network ls'
                // sh 'docker-compose down'
                sh 'docker network disconnect -f appnetwork application-up_aplication_1'
                sh 'docker network disconnect -f appnetwork application-up_bd-postgres_1'
                sh 'docker network rm appnetwork'
                sh 'docker stop application-up_aplication_1 application-up_bd-postgres_1'
                sh 'docker rm application-up_aplication_1 application-up_bd-postgres_1'
            }
        }
    }
}
