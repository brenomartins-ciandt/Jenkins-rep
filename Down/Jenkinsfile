pipeline {
    agent any
    
	stages {
        stage ('Compose Stage'){
            steps {
                sh "ls"
                sh 'pwd'
                sh 'docker network ls'
                // sh 'docker-compose down'
                // sh 'docker network ls'
                sh 'docker ps'
                //sh 'docker network disconnect netjenkins jenkinscontainer'
                sh 'docker network disconnect netjenkins aplication-up_aplication_1'
                sh 'docker network disconnect netjenkins aplication-up_bd-postgres_1'
                sh 'docker network ls'
               // sh 'docker network rm netjenkins'
                sh 'docker stop aplication-up_aplication_1 aplication-up_bd-postgres_1'
                sh 'docker rm aplication-up_aplication_1 aplication-up_bd-postgres_1'
                sh 'docker network ls'
                sh 'docker ps'
                sh 'docker ps -a'
            }
        }
    }
}
